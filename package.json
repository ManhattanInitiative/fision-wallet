{
  "name": "fision-wallet",
  "version": "1.0.0",
  "displayName": "Fision Wallet for Fuel Network",
  "author": "ManhattanInitiative",
  "description": "Access Fuel with Fision",
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch",
    "dev": "vite",
    "serve:firefox": "web-ext run --start-url \"about:debugging#/runtime/this-firefox\"  --source-dir ./dist/",
    "serve:chrome": "web-ext run -t chromium --start-url \"https://google.com\" --source-dir ./dist/",
    "start:firefox": "concurrently \"npm:dev\" \"npm:serve:firefox\" ",
    "start:chrome": "concurrently \"npm:dev\" \"npm:serve:chrome\" "
  },
  "devDependencies": {
    "@samrum/vite-plugin-web-extension": "^1.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/download": "^8.0.1",
    "@types/webextension-polyfill": "^0.8.3",
    "autoprefixer": "^10.4.7",
    "body-parser": "^1.20.0",
    "concurrently": "^7.2.1",
    "download": "^8.0.0",
    "express": "^4.18.1",
    "postcss": "^8.4.14",
    "safe-compare": "^1.1.4",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "vite": "~2.9.0",
    "vite-plugin-solid": "^2.2.6",
    "web-ext": "^6.6.0"
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
    "fuels": "^0.6.0",
    "qrcode": "^1.5.0",
    "solid-app-router": "^0.3.3",
    "solid-js": "^1.3.13",
    "webext-bridge": "^5.0.4",
    "webextension-polyfill": "^0.9.0"
  },
  "type": "module"
}
